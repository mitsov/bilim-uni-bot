    $Unmute = (включи*)

    $Sound = (громкост*|звук|говори*)

    $Set = (*став*|установи*|сдела*|переключи*|включи*|говори|пой|ключи|вруб*)
    $Unset = (выруби*|выключи*|отключи*|отмени*|выруб*|выключ*|отключ*|отмен*)


    $stopGame = (* $stopGameStrong */$stopGameWeak)

    $stopGameStrong = (давай (хватит/закончим)/хватит играть|надоело|{больше не (хочу/хочется)}|прекращ*|прекрат*|стоп|останов*|заканч*|[давай] законч* [игр*]|устал* игр*|{не (хочу/хочется) [больше] играть}/надоел/надоела/мне надоело играть/{(не нравится) * игра}/(выйти/выход) из * игры)

