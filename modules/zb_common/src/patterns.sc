require: text/text.sc
require: phoneNumber/phoneNumber.sc
patterns:

    $yes = [ну] [конечно|всё|все|вроде|пожалуй|возможно] (да|даа|lf|ага|агась|точно|угу|верно|ок|ok|окей|окай|okay|оке|именно|подтвержд*|йес) [да|конечно|конешно|канешна|всё|все|вроде|пожалуй|возможно]

    $agree = ({[$yes|конечно|конешно|канешна] (* $agreeStrong *|$agreeWeak)}|$repeat<$yes>|{($yes|конечно|конешно|канешна) $try})

    $agreeWeak = (давай|давайте|логично|могу|было дело|бывало|бывает)

    $agreeStrong = (конечно|конешно|канешна|а то [нет]|очень|[ты] прав*|абсолютно|обязательно|непременно|а как же|[я] подтверждаю|[совершенно|абсолютно] точно|пожалуй|запросто|норм|(почему/что/че) [бы] [и] нет|хочу|было [бы] (неплохо|не плохо)|[я] [очень] (хочу|хо чу|ладно|хорошо)|ладно|можно|валяй*|договорились|[я] [совершенно|абсолютно] [с (тобой/вами)] согла*|не могу [с (тобой/вами)] не согласиться|вполне|в полной мере|естественно|разумеется|(еще|ещё) как|[я] не (против|возражаю|сомневаюсь)|я только за|безусловн*|[это] так [и есть]|все (так|верно)|(совершенно|абсолютно) верно)

    $no = (нет|неат|ниат|неа|ноуп|ноу|найн) [нет] [спасибо]

    $disagree = (* $disagreeStrong *|$disagreeWeak|$no)
    
    $disagreeWeak = (да ну|не|нее|ничего)

    $disagreeStrong = [$no|конечно|конешно|канешна] ($no|не сейчас/ни капли/отнюдь/нискол*/да ладно/[я] не (хоч*|хо чу|надо|могу|очень|думаю|нравится|стоит|буду|считаю|согла*|подтв*)|ненадо|нельзя|нехочу|ненавижу|невозможно|никогда|никуда|ни за что|нисколько|никак*|никто|ниразу|[я] против|вряд ли|сомневаюсь|нихрена|неправильно|неверно|невсегда|[это] {не так}|отказываюсь|ни в коем случае) [(конечно|конешно|канешна|спасибо)] [$compliment]

    $try = {попроб* [$agreeStrong] ([еще] [раз|разок|разик])}

    $andYou = (а {([у] $you) [как*]}/[а] {(у тебя) [как*]})

    $AnyWord = (а*|б*|в*|г*|д*|е*|ё*|ж*|з*|и*|й*|к*|л*|м*|н*|о*|п*|р*|с*|т*|у*|ф*|х*|ц*|ч*|ш*|щ*|ъ*|ы*|ь*|э*|ю*|я*|да*|не*|a*|b*|c*|d*|e*|f*|g*|h*|i*|j*|k*|l*|m*|n*|o*|p*|q*|r*|s*|t*|u*|v*|w*|x*|y*|z*)

    $bad = [(очень|весьма|как-то|что-то)] (грустн*|плох*|хренов*|херов*|ужас*|невыносим*|ниочём|хуёво|не очень|неочень|отстой*|отврат*|нехорош*|не $good|мерзк*)

    $bot = (бот|ботик|робот*|чатбот|чат бот|чат-бот|сервис|чатик|~чат|[искус*] интеллект*)

    $bye = ([(ладно|все|давай|ну)] (пока [пока]|(до|да) (свидан*|встречи|завтра|скорого [свидания]|скорой встречи)|досвидани*|досвидос|дозавтра|прощай|бай|бай бай|я (пошел|пошла|ушла|отошел|отошла|отойду|уйду|ушел|ухожу|пойду)|гудбай|чау|чао|poka|чмоки чмоки|пока-пока|покедова|покеда|покасики|п о к а|мне (пора|нужно) (~идти|*спать|*учиться|*работать|на *|в *|уходить)|всего доброго|не поминай* лихом|адью|адье))

    $cannot = (не выходит|никак|не могу|не можем|не могут|немогу|немагу|неможем|не получа*|не удается|не удаётся|невозмож*|не возмож*|не дает)
    
    $catchAll = *

    $changeTheme = (не хочу (об этом|про это) [разговаривать|говорить]|не тво* дело|не приставай с * вопросами|[мне] нет {(до этого) дела}|[$you] [$me] (достала|достал)|(смени*|поменя*) тему) [$compliment]    

    $clever = {[$you] [очень] (умна*|умны*|умница|умничка|умненьк*|молоде*|маладе*|хороший|мало детс|мо ло дец|лапочка|няша|гений|~гениальный)}

    $compliment = ([$you] [(очень|так*|сегодня|просто|моя)] (~красивый|молодец|~классный|~прекрасный|красав*|супер|красотк*|подруж*|детк*|~сексуальный|секси|сексе|~милый|милаха|прелесть|{$good выглядишь}|солнышк*|зайк*|зая|~отличный|~отпадный|~замечательный|~крутой|крутая|~потрясающий|~потрясный|потрясная|~обворожителый|~несравненный|головокружител*|~клевый|~бесподобный|бесподобная|~прикольный|прикольная|~симпатичный|~хорошенький|хорошенькая|~добрый|~ласковый|милаш|милашка|~привлекательный|~ахуенный|~очаровательный|~великолепный|~ослепительный|~сногшибательный|~шикарный|молодец*|молодц*|молодчин*|~чудесный|волшебн*|[настоящее] золото)|платье * огонь|поздравля*)

    $dontKnow = [([я] даже|ну [я]|сам*)] ((не|откуда $me) (зна*|задумывал*|думал*|решил*|представляю|уверен*|помню)|незнаю|неизвестно|надо подумать|без понятия|без понятий|понятия не имею|сомневаюсь|сложны* вопрос*|сложно сказать|все сложно|ну ты [и]спросил|спросишь тоже|забыл*/сдаюсь|хз|(фиг|хрен) [$AnyWord] знает)

    $five = (5|пят*|пятер*|пята*|five*|fifth)

    $four = (4|четыр*|четв*|four*)

    $gender = (
        (мальч*|парень|муж*|не (дев*|жен*)):0|
        (дев*|жен*|не (дев*|мальчик|муж*)):1)

    $good = [(очень|так*|конечно|всё|все|просто)] (добр*|хорошо|хорош*|отличн*|классн*|крут*|замечательн*|потряс*|великолепн*|прекрасн*|прекрасно|заебись|зашибись|[о] кей|здорово|здорого|порядок|в порядке) [(очень|так*|конечно|всё|все|просто)]

    $goodNight = {[$bye] (я [пошел] спать|(спокой*/спокойной|~добрый) ночи|(сладких|приятных) снов|споки|споки-споки|давай {спать будем})}

    $hello = (привет* [привет]|привеет:привет|приивеет:привет|приветик|приветики|при вед:привед|здравствуй|здравствуй*|здрасьте|здрасти|high:хэй|эй:хэй|хай|хэлло*|халоу|хэлоу|здаров|шалом|hello|здарова|хелло|здаровеньки|здоровеньки|здрасте|салют|hi|приветствую|~добрый ~день|~добрый ~вечер|доброго времени суток|хеллоу|п р и в е т|здраствуй*|приве:привет|прив:привет|хаюшки)

    $inf = $morph<ИНФИНИТИВ>

    $like = (симпатичн*/симпатизируешь/симпатизируете/предпочитаешь/предпочитаете/привле*/привлекает/привлекательн*/импонирует/импонируют/нравится/нравятся/~любимый/любит*/любишь/любите/по душе)

    $looser = [$you] ($stupid|(пошла|иди|пиздуй) (в|на) (жопу|хуй|пизду|хер|хрен)|неудачница|неудачник|убейся|удались|(убей|удали) себя|лузер*|лох*|лошара|ламер*|хаха*|ха-ха*|ахаха*|сам* виноват*|истери*|лохушка)

    $maybe = {(может [быть]|наверн*|возможн*|вероятн*|скорее всего|вроде [бы|да]) [тогда]} [$compliment]

    $me = [$preposition] (я|меня|мне|мной|мой|моя|мою|мое*|моим|моём|моей|себ*|мня/мну)

    $my = (мой|моя|мое|моё|мае|маё|мне|мои|маи|мою|маю|моне|мане|манё|монё|меня|миня|моих|маих|моим|маим|моем|маем|моём|маём|мя|ма|мня|[со] мной)  

    $need = (нужен|необходим*|нужно|надо*|нужны|нужна|требует*|~требовать)

    $normal = (нормальн*|так себе|не $bad|норм|неплохо|пойдет|сойдет|нормас)

    $notNow = (не [могу] сейчас|мне (некогда|не до *того)|(в другой|не в этот) раз|[давай] (*позже|потом|не сегодня|не сейчас|пока не надо) [поговорим]|нет времени|я (занят|занята)) [$compliment]

    $noun = $morph<С>

    $now = (сейчас|сча|счас|щас|ща|только что|теперь|[сей] [же] (час|миг|момент))

    $one = (1|первы*|перво*|один|одн*|одна|едини*|one|first)

    $pls = (пожалуйста|пожалста|пжалста|пжлста|плиз|плз)

    $preposition = (у|про|для|за|из [за]|из-за|в|без|до|через|с|со|об|от|к|ко|о|обо|об|при|по|на|с|над|под|перед)

    $question = (как*|(что|что же|чтоже|чего|что за) [так*]|почему|по че му|зачем|где|кто [так*]|чей|который|сколько|скока|кем|[о] чем|че|чё|[в] смысле|очем|почем|o ком|когда|куда) [$preposition]

    $seven = (7|семь|седьм*|семер*|seven|seventh)

    $sing = (пой|спой|споешь|споёшь|споем|~петь|попеть|спеть|пропеть|пропой|пропоем|споем)

    $six = (6|шест*|шестер*|six|sixth)

    $stupid = [ну и] (~тупой|тупен*|~глупый|имбецил*|даун*|дура|дуру|дурында|(ду ра):дура|тупая [(блондинка|дура)]|тупишь|туповат|дурочк*|*дурак*|придурок|дурач*|дуроч*|идиот*|тупиц*|балбес|глуп*|куриц*|дебил*|дебилоид*|долбоёб|кретин*|балда|бестолочь|неудачни*|тупл*|тупорылая|хуйл*|лузер*|лох*|пидор*|лошара|ламер*|мудак*|мудил*|сволочь|козел|козл*|уебок|склерот*|ёбнутая|ёбнутый|(тупе*) [на глазах]|чукча) [тупая|тупой] [(что ли|чтоли)]

    $super = (крут*|супер*|ура|wow|классн*|интересн*|прикольн*|угарн*|ух ты|ого|отлично|офиген*) [$compliment]

    $sure = (конечно|канешна|конешно|безусловно|очевидно|логично|точно) [же]

    $tellMe = (расскажи|рассказывай|валяй|покажи|покажешь|скажи|поговорим|давай ты мне расскажешь|можешь [мне] рассказать) [мне] [пожал*] [$preposition]

    $thanks = (спасибо [и] [тебе|вам] [большое]|[премного] благодар*|спс|супер|отлично|молод*|умни*|пасиб*)

    $three = (3|трет*|три|тройк*|троечк*|трояк|third|three)

    $turnOff = ([хочу] выключ*|отключ*|закр*|выруб*|стоп|останови*)

    $turnOn = [хочу|можешь] (включ*|откр*|ключи*|постав*|ключи)  [мне]

    $two = (2|второ*|два|двойк*|двоечк*|two|second)

    $what = (что|чё|чо|шо|чего|чаво|чиво|чиго)

